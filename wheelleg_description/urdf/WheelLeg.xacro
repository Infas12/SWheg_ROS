<?xml version="1.0" encoding="utf-8"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

  <!--left wheelLeg model-->
  <xacro:macro name="LeftWheelLeg" params="name x y z parent">

    <link name="${name}">
      <inertial>
        <origin
          xyz="4.8179E-09 1.5313E-06 0.045042"
          rpy="0 0 0" />
        <mass
          value="0.075119" />
        <inertia
          ixx="0.00010565"
          ixy="-1.0649E-07"
          ixz="-2.3479E-12"
          iyy="2.7546E-05"
          iyz="-8.9308E-10"
          izz="9.6655E-05" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/Left.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="1 1 1 1" />
        </material>
      </visual>
      <collision>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/Left.STL" />
        </geometry>
      </collision>
    </link>

    <joint name="${name}_Joint" type="continuous">
      <origin
        xyz="${x} ${y} ${z}"
        rpy="1.5708 -1.5708 3.1416" />
      <parent
        link="${parent}" />
      <child
        link="${name}" />
      <axis
        xyz="0 0 1" />
      <limit effort="2.8" velocity="30"/>
    </joint>

    <link name="${name}RA">
      <inertial>
        <origin
          xyz="0.060620953877514 -0.0509052185223456 -0.0160797180106055"
          rpy="0 0 0" />
        <mass
          value="0.102065831041357" />
        <inertia
          ixx="0.000107618762660542"
          ixy="6.4058045299092E-05"
          ixz="1.40721471251035E-07"
          iyy="0.000426511611787524"
          iyz="-2.25569567809916E-07"
          izz="0.000513500369211842" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/LeftRimA.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="1 1 1 1" />
        </material>
      </visual>
      <collision name = "${name}RA_collision">
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/LeftRimA.STL" />
        </geometry>
      </collision>
    </link>

    <joint name="${name}RA_Joint" type="continuous">
      <origin      
        xyz="0 -0.08 0.038242"
        rpy="3.1416 0 1.5708" />
      <parent
        link="${name}" />
      <child
        link="${name}RA" />
      <axis
        xyz="0 0 1" />
      <dynamics damping="0.01" friction="0.0"/>
      <limit effort="2.8" velocity="5"/>
    </joint>

    <link name="${name}RB">
      <inertial>
        <origin
          xyz="0.060620953877514 -0.0509052185223458 -0.0160797180106054"
          rpy="0 0 0" />
        <mass
          value="0.102065831041357" />
        <inertia
          ixx="0.000107618762660542"
          ixy="6.4058045299092E-05"
          ixz="1.40721471251074E-07"
          iyy="0.000426511611787524"
          iyz="-2.2556956780994E-07"
          izz="0.000513500369211842" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/LeftRimB.STL" />
        </geometry>
        <material
          name="">
          <color
            rgba="1 1 1 1" />
        </material>
      </visual>
      <collision name = "${name}RB_collision">
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://wheelleg_description/meshes/LeftRimB.STL" />
        </geometry>
      </collision>
    </link>

    <joint name="${name}RB_Joint" type="continuous">
      <origin
        xyz="0 0.08 0.038242"
        rpy="3.1416 0 -1.5708" />
      <parent
        link="${name}" />
      <child
        link="${name}RB" />
      <axis
        xyz="0 0 1" />
      <dynamics damping="0.01" friction="0.0"/>
      <limit effort="2.8" velocity="5"/>
    </joint>

  </xacro:macro>


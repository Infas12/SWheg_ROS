<launch>
    <!-- imu型号，默认 normal -->
    <arg name="type" default="normal" doc="type [normal, modbus]"/>

    <!-- imu 对应 python 文件 -->
    <node pkg="wheelleg_real" type="wit_$(arg type)_ros.py" name="imu" output="screen">
        <param name="port"               type = "str"    value="/dev/ttyUSB1"/>
        <param name="baud"               type = "int"    value="115200"/>
    </node>

    <!-- <node pkg="wheelleg_real" type="imu_collect.py" name="imu_collect" ></node> -->
</launch>

